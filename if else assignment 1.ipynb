    {
      "cell_type": "code",
      "source": [
        "num = int(input(\"Enter the number:\"))\n",
        "if num%2==0:\n",
        "  print(\"{} is even\".format(num))\n",
        "else:\n",
        "  print(\"{} is odd\".format(num))\n",
        "    "
